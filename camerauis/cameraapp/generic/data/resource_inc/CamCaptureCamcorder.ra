            bitmaps =
                {
                CAMERA_BITMAP { bitmap_id = EMbmCameraappQgn_indi_vid4_stabilize_on; },
                CAMERA_BITMAP { bitmap_id = EMbmCameraappQgn_indi_vid4_stabilize_on_mask; }
                };
            },
        CAMERA_INDICATOR
            {
            indicator_id = ECamIndicatorAudioMute;
            coords =
                {
                CAMERA_INDICATOR_COORDS
                    { tl_x = 0; 
                      tl_y = 0; 
                      br_x = 0; 
                      br_y = 0; }
                };
            bitmaps =
                {
