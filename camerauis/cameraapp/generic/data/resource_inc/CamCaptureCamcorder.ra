                CAMERA_BITMAP { bitmap_id = EMbmCameraappQgn_indi_vid4_audio_mute_mask; }
                };
            },
        CAMERA_INDICATOR
            {
            indicator_id = ECamIndicatorFaceTracking;
            coords =
                {
                CAMERA_INDICATOR_COORDS
                    { tl_x = 0; 
                      tl_y = 0; 
                      br_x = 0; 
                      br_y = 0; }
                };
            bitmaps =
