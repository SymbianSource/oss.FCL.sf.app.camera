         {
         CAM_ACTIVE_PALETTE_ITEM
             {
             item_id  = ECamSettingItemImageModeSelect; 
             cmd_id   = ECamCmdNewPhoto;
             position = ECamAPSlotVideoPreCaptureMode;
             type     = ECamActivePaletteItemTypePreCaptureNonSetting;
             },
